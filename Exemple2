public class Exemple2 {
    public static void main (String args[]){
        int num; //в этой строке кода объявлется переменная с именем num

        num =100;

        System.out.println("Это ппеременная num: "+num);

        num = num*2;

        System.out.println("значение епременной num * 2 равно ");
        System.out.println(num);
    }
}
